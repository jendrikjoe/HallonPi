namespace tcp{


class CommunicationPackage{

public:
	void getFrame(char *output) const;
	unsigned int getTimeStamp() const;
	bool isValid() const;
	bool isStarting(char *input);

protected:
	CommunicationPackage();
	~CommunicationPackage();
};

}
