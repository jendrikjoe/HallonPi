#include "HousekeepingCommunicationPackage.h"

namespace tcp {

const unsigned short packageLength = 14;
const PackageType = HOUSE_COM;


HousekeepingCommunicationPackage::HousekeepingCommunicationPackage() :
CommunicationPackage() {
	this->cpuAverage = 0;
}

HousekeepingCommunicationPackage::HousekeepingCommunicationPackage(char* frame) :
	CommunicationPackage(frame, packageType, ){
	if(!valid) {
		this->cpuAverage = 0;
	}
	else  {
		this->cpuAverage = readCpuAverage();
	}
}

HousekeepingCommunicationPackage::HousekeepingCommunicationPackage(unsigned short cpuAverage)

HousekeepingCommunicationPackage::~HousekeepingCommunicationPackage() {
	// TODO Auto-generated destructor stub
}

}
