#include "Thread.h"

Thread::Thread() {
}

Thread::~Thread() {}

void* Thread::startThread(void* pVoid) {
	Thread* aThread = static_cast<Thread*>(pVoid);
	assert(aThread);
	aThread->result = aThread->run();
	aThread->setCompleted();
	return aThread->result;
}

